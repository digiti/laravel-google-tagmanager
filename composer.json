{
    "name": "digiti/laravel-google-tagmanager",
    "description": "Laravel Google Shopping Feed",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Digiti\\GoogleTagmanager\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "spatie/array-to-xml": "2.8.*|3.*.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digiti\\GoogleTagmanager\\GoogleTagmanagerServiceProvider"
            ],
            "aliases": {
                "GoogleTagmanager": "Digiti\\GoogleTagmanager\\GoogleTagmanagerFacade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.28"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
